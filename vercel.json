{
  "version": 2,
  "builds": [
    {
      "src": "home/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "quicklook/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/quicklook/_next(.*)",
      "dest": "quicklook/_next$1"
    },
    {
      "src": "/quicklook(.*)",
      "dest": "quicklook/quicklook$1"
    },
    {
      "src": "(.*)",
      "dest": "home$1"
    }
  ]
}