{
    "version": 2,
    "builds": [
      { "src": "./package.json", "use": "@vercel/next" },
      { "src": "packages/quicklook/package.json", "use": "@vercel/next" }
    ],
    "routes": [
      { "src": "/blog/_next(.*)", "dest": "blog/_next$1" },
      { "src": "/blog(.*)", "dest": "blog/blog$1" },
      { "src": "(.*)", "dest": "home$1" }
    ]
  }